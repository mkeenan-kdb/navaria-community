{
  "expo": {
    "name": "Navaria",
    "slug": "navaria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/expo-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.navaria.app",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "ITSAppUsesNonExemptEncryption": false
      },
      "appleTeamId": "FQ2T85NLA2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.navaria.app",
      "permissions": [
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "baseUrl": "/app"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ],
      "./plugins/withPodfilePostInstall.js",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/MeathFLF.ttf",
            "./assets/fonts/ardchlo.otf",
            "./assets/fonts/bungc.otf",
            "./assets/fonts/gear.otf",
            "./assets/fonts/glangc.otf",
            "./assets/fonts/mearchlo.otf",
            "./assets/fonts/mionchlo.otf",
            "./assets/fonts/SraidchloGC-Regular.otf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fffffe",
          "image": "./assets/splash-icon-v2.png",
          "resizeMode": "contain",
          "imageWidth": 200
        }
      ],
      "expo-audio",
      "expo-video"
    ],
    "scheme": "navaria"
  }
}
